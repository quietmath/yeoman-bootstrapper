{
  "name": "@quietmath/generator-yo-bootstrapper",
  "version": "0.1.1",
  "description": "Node.JS project generator based on Quiet Math project templates.",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/example/**/*.js",
      "**/example/**/*.ts",
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "nodejs",
    "typescript"
  ],
  "files": [
    "generators"
  ],
  "contributors": [
    "Michael Szul <michael@szul.us>"
  ],
  "dependencies": {
    "fs-extra": "^9.0.0",
    "yeoman-generator": "^4.9.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/yeoman-generator": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "mocha": "^8.1.1",
    "nyc": "^15.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "yo": "^3.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quietmath/yeoman-bootstrapper.git"
  },
  "bugs": {
    "url": "https://github.com/quietmath/yeoman-bootstrapper/issues"
  },
  "homepage": "https://github.com/quietmath/yeoman-bootstrapper#readme",
  "publishConfig": {
    "access": "public"
  }
}
